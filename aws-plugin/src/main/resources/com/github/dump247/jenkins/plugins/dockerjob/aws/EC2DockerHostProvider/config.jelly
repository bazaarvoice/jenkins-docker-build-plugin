<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form">

    <f:entry title="Filter" field="filterString">
        <f:textbox/>
    </f:entry>

    <f:entry title="Regions" field="regionString">
        <f:textbox default="us-east-1"/>
    </f:entry>

    <f:entry title="Use" field="usePublicIP">
        <f:booleanRadio true="Public IP" false="Private IP"/>
    </f:entry>

</j:jelly>